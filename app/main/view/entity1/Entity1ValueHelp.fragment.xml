<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="NO DATA"
		title="Select entity1"
		search="onEntity1ValueHelpDialogSearch"
		confirm="onEntity1ValueHelpDialogConfirm"
		cancel="onEntity1ValueHelpDialogClose"
        rememberSelections="true"
		items="{
			path: 'entity>/Entity1',
			sorter: {
				path: 'ID',
				descending: false
			}
		}">
		<StandardListItem
			title="{entity>title}"
			description="{entity>descr}"/>
	</SelectDialog>
</core:FragmentDefinition>